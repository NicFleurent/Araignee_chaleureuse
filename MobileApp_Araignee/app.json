{
  "expo": {
    "name": "Araignée chaleureuse",
    "slug": "Araignée chaleureuse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/spider-bot.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/spider-bot.png",
      "resizeMode": "contain",
      "backgroundColor": "#84DCC6"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/spider-bot.png",
        "backgroundColor": "#84DCC6"
      },
      "package": "com.anonymous.MobileApp_Araignee"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-sqlite",
      "expo-localization"
    ]
  }
}
